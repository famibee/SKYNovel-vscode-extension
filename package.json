{
	"name": "skynovel",
	"version": "3.3.13",
	"publisher": "famibee",
	"engines": {
		"vscode": "^1.50.0"
	},
	"license": "MIT",
	"displayName": "SKYNovel",
	"description": "It supports development of SKYNovel for VSCode, VSCodeでSKYNovelの開発をサポートする拡張機能",
	"categories": [
		"Programming Languages",
		"Snippets",
		"Debuggers",
		"Other"
	],
	"keywords": [
		"skynovel",
		"vscode",
		"vscode-extension",
		"vscode-plugin",
		"typescript",
		"multi-root ready"
	],
	"main": "extension.js",
	"contributes": {
		"languages": [
			{
				"id": "skynovel",
				"aliases": [
					"SKYNovel"
				],
				"extensions": [
					".sn",
					".an",
					".ks"
				],
				"configuration": "res/language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "skynovel",
				"scopeName": "source.skynovel",
				"path": "res/sn.tmLanguage.json"
			}
		],
		"breakpoints": [
			{
				"language": "skynovel"
			}
		],
		"debuggers": [
			{
				"type": "skynovel",
				"label": "SKYNovel Debug",
				"languages": [
					"skynovel"
				],
				"configurationAttributes": {
					"attach": {
						"properties": {
							"userDataDir": {
								"type": "string",
								"description": "Absolute path to App Data.",
								"default": "${workspaceFolder}/.vscode/chrome"
							},
							"stopOnEntry": {
								"type": "boolean",
								"description": "Automatically stop after launch.",
								"default": true
							}
						}
					},
					"launch": {
						"properties": {
							"userDataDir": {
								"type": "string",
								"description": "Absolute path to App Data.",
								"default": "${workspaceFolder}/.vscode/chrome"
							},
							"stopOnEntry": {
								"type": "boolean",
								"description": "Automatically stop after launch.",
								"default": true
							}
						}
					}
				}
			}
		],
		"commands": [
			{
				"command": "skynovel.refreshSetting",
				"title": "Refresh",
				"icon": {
					"light": "res/light/refresh.svg",
					"dark": "res/dark/refresh.svg"
				}
			},
			{
