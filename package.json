		"viewsContainers": {
			"activitybar": [
				{
					"id": "skynovel",
					"title": "SKYNovel",
					"icon": "res/img/icon-activity-bar.svg"
				}
			]
		},
		"views": {
			"skynovel": [
				{
					"id": "sn-setting",
					"name": "開発環境"
				},
				{
					"id": "sn-dev",
					"name": "開発ツール"
				},
				{
					"id": "sn-doc",
					"name": "ドキュメント・連絡先"
				}
			]
		},
		"menus": {
			"view/title": [
				{
					"command": "skynovel.refreshSetting",
					"when": "view == sn-setting",
					"group": "navigation"
				}
			],
			"view/item/context": [
				{
					"group": "inline",
					"command": "skynovel.dlNode",
					"when": "view == sn-setting && viewItem == 'Node.js'"
				},
				{
					"group": "inline",
					"command": "skynovel.opNodeSite",
					"when": "view == sn-setting && viewItem == 'Node.js サイト'"
				},
				{
					"group": "inline",
					"command": "skynovel.devPrjSet",
					"when": "view == sn-dev && viewItem == 'skynovel.devPrjSet'"
				},
				{
					"group": "inline",
					"command": "skynovel.devSnUpd",
					"when": "view == sn-dev && viewItem == 'skynovel.devSnUpd'"
				},
				{
					"group": "inline",
					"command": "skynovel.devLibUpd",
					"when": "view == sn-dev && viewItem == 'skynovel.devLibUpd'"
				},
				{
					"group": "inline",
					"command": "skynovel.devCrypto",
					"when": "view == sn-dev && viewItem == 'skynovel.devCrypto'"
				},
				{
					"group": "inline",
					"command": "skynovel.devReBuild",
					"when": "view == sn-dev && viewItem == 'skynovel.devReBuild'"
				},
				{
					"group": "inline",
					"command": "skynovel.devTaskWeb",
					"when": "view == sn-dev && viewItem == 'skynovel.devTaskWeb'"
				},
				{
					"group": "inline",
					"command": "skynovel.devTaskApp",
					"when": "view == sn-dev && viewItem == 'skynovel.devTaskApp'"
				},
				{
					"group": "inline",
					"command": "skynovel.devPackWin",
					"when": "view == sn-dev && viewItem == 'skynovel.devPackWin'"
				},
				{
					"group": "inline",
					"command": "skynovel.devPackWin32",
					"when": "view == sn-dev && viewItem == 'skynovel.devPackWin32'"
				},
				{
					"group": "inline",
					"command": "skynovel.devPackMac",
					"when": "view == sn-dev && viewItem == 'skynovel.devPackMac'"
				},
				{
					"group": "inline",
					"command": "skynovel.devPackLinux",
