				{
					"id": "sn-setting",
					"name": "開発環境"
				},
				{
					"id": "sn-ws",
					"name": "ワークスペース"
				},
				{
					"id": "sn-tb",
					"name": "ツールボックス",
					"type": "webview"
				},
				{
					"id": "sn-doc",
