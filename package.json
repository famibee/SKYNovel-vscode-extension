	"devDependencies": {
		"@types/archiver": "^3.1.1",
		"@types/crypto-js": "^4.0.1",
		"@types/fs-extra": "^9.0.2",
		"@types/mocha": "^8.0.3",
		"@types/node": "^14.14.2",
		"@types/uuid": "^8.3.0",
		"@types/vscode": "^1.50.0",
		"ts-loader": "^8.0.6",
		"typescript": "^4.0.3",
		"vscode-debugadapter-testsupport": "^1.42.0",
		"vscode-test": "^1.4.0",
		"webpack": "^4.44.2",
		"webpack-cli": "^3.3.12"
	},
	"scripts": {
		"build": "webpack --mode development --config ./core/webpack.config.js",
		"watch": "webpack --mode development --watch --config ./core/webpack.config.js",
		"npm_check_updates_do": "ncu -a && npm update && npm i -D",
		"version:patch": "npm -no-git-tag-version version patch",
		"version:minor": "npm -no-git-tag-version version minor",
		"version:major": "npm -no-git-tag-version version major",
		"publish": "vsce package",
		"vscode:prepublish": "npm run md2json && npm run wp:pro",
		"wp:pro": "webpack -p --config ./core/webpack.config.js",
		"md2json": "node core/lib/md2json.js",
		"md2json_bld": "tsc -p core/tsconfig_md.json",
		"rebuild": "rm -rf node_modules && rm package-lock.json && npm cache clear --force && npm i && npm run md2json_bld"
	},
	"icon": "res/img/icon_skynovel.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/famibee/SKYNovel-vscode-extension.git"
	},
	"bugs": {
