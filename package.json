{
	"name": "skynovel",
	"displayName": "SKYNovel",
	"description": "It supports development of SKYNovel for VSCode, VSCodeでSKYNovelの開発をサポートする拡張機能",
	"version": "0.0.3",
	"author": "miyazawa famibee",
	"publisher": "famibee",
	"engines": {
		"vscode": "^1.30.0"
	},
	"categories": [
		"Programming Languages",
		"Other"
	],
	"icon": "images/icon.png",
	"activationEvents": [
		"onLanguage:skynovel"
	],
	"main": "./out/extension.js",
	"contributes": {
		"languages": [
			{
				"id": "skynovel",
				"aliases": ["SKYNovel"],
				"extensions": [
					".sn",
					".an"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "skynovel",
				"scopeName": "source.skynovel",
				"path": "./syntaxes/skynovel.tmLanguage.json"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test",
		"version:patch": "npm -no-git-tag-version version patch",
		"version:minor": "npm -no-git-tag-version version minor",
		"version:major": "npm -no-git-tag-version version major",
		"package": "vsce package"
	},
	"devDependencies": {
		"typescript": "^3.1.4",
		"vscode": "^1.1.25",
		"@types/node": "^8.10.25",
		"@types/mocha": "^2.2.42"
	},
	"keywords": [
		"skynovel",
		"vscode",
		"vscode-extension",
		"vscode-plugin",
		"typescript"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/famibee/SKYNovel-vscode-extension.git"
	},
	"bugs": {
		"url": "https://github.com/famibee/SKYNovel-vscode-extension/issues",
		"email": "famibee@gmail.com"
	},
	"homepage": "http://famibee.blog38.fc2.com/"
}
