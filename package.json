{
	"name": "skynovel",
	"displayName": "SKYNovel",
	"description": "It supports development of SKYNovel for VSCode, VSCodeでSKYNovelの開発をサポートする拡張機能",
	"version": "1.6.4",
	"author": "miyazawa famibee",
	"publisher": "famibee",
	"license": "MIT",
	"engines": {
		"vscode": "^1.35.1"
	},
	"categories": [
		"Programming Languages",
		"Other"
	],
	"icon": "res/icon.png",
	"activationEvents": [
		"onLanguage:skynovel",
		"onCommand:skynovel.openReferencePallet",
		"workspaceContains:prj/prj.json"
	],
	"main": "core/lib/extension.js",
	"contributes": {
		"languages": [
			{
				"id": "skynovel",
				"aliases": [
					"SKYNovel"
				],
				"extensions": [
					".sn",
					".an"
				],
				"configuration": "res/language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "skynovel",
				"scopeName": "source.skynovel",
				"path": "res/sn.tmLanguage.json"
			}
		],
		"commands": [
			{
				"command": "skynovel.openReferencePallet",
				"title": "%skynovel.openReferencePallet.title%",
				"category": "SKYNovel"
			},
			{
				"command": "skynovel.refreshSetting",
				"title": "Refresh",
				"icon": {
					"light": "res/light/refresh.svg",
					"dark": "res/dark/refresh.svg"
				}
			},
			{
				"command": "skynovel.dlNode",
				"title": "Download Node.js",
				"icon": {
					"light": "res/light/download.svg",
					"dark": "res/dark/download.svg"
				}
			},
			{
				"command": "skynovel.opNodeSite",
				"title": "Open Node.js Download WebPage",
				"icon": {
					"light": "res/light/openpage.svg",
					"dark": "res/dark/openpage.svg"
				}
			},
			{
				"command": "skynovel.devSnUpd",
				"title": "Update SKYNovel",
				"icon": {
					"light": "res/light/download.svg",
					"dark": "res/dark/download.svg"
				}
			},
			{
				"command": "skynovel.devCrypt",
				"title": "Encryption switching",
				"icon": {
					"light": "res/light/play.svg",
					"dark": "res/dark/play.svg"
				}
			},
			{
				"command": "skynovel.devTaskWeb",
				"title": "npm web (on Browser)",
				"icon": {
					"light": "res/light/play.svg",
					"dark": "res/dark/play.svg"
				}
			},
			{
				"command": "skynovel.devTaskStart",
				"title": "npm start (on Electron)",
				"icon": {
					"light": "res/light/play.svg",
					"dark": "res/dark/play.svg"
				}
			},
			{
				"command": "skynovel.devTaskPackWin",
				"title": "npm pack:win (for Windows Package)",
				"icon": {
					"light": "res/light/baggage.svg",
					"dark": "res/dark/baggage.svg"
				}
			},
			{
				"command": "skynovel.devTaskPackMac",
				"title": "npm pack:mac (for macOS Package)",
				"icon": {
					"light": "res/light/baggage.svg",
					"dark": "res/dark/baggage.svg"
				}
			},
			{
				"command": "skynovel.opDev",
				"title": "Open Information for developers WebPage",
				"icon": {
					"light": "res/light/openpage.svg",
					"dark": "res/dark/openpage.svg"
				}
			},
			{
				"command": "skynovel.opTag",
				"title": "Open Tag Reference",
				"icon": {
					"light": "res/light/openpage.svg",
					"dark": "res/dark/openpage.svg"
				}
			},
			{
				"command": "skynovel.opMacroPlg",
				"title": "Open Macro And Plugin Reference",
				"icon": {
					"light": "res/light/openpage.svg",
					"dark": "res/dark/openpage.svg"
				}
			},
			{
				"command": "skynovel.opGallery",
				"title": "Open Sample Gallery",
				"icon": {
					"light": "res/light/openpage.svg",
					"dark": "res/dark/openpage.svg"
				}
			},
			{
				"command": "skynovel.dlTmpYoko",
				"title": "Download Template",
				"icon": {
					"light": "res/light/download.svg",
					"dark": "res/dark/download.svg"
				}
			},
			{
				"command": "skynovel.dlTmpTate",
				"title": "Download Template",
				"icon": {
					"light": "res/light/download.svg",
					"dark": "res/dark/download.svg"
				}
			},
			{
				"command": "skynovel.opFamibeeBlog",
				"title": "Open famibee WebPage",
				"icon": {
					"light": "res/light/openpage.svg",
					"dark": "res/dark/openpage.svg"
				}
			},
			{
				"command": "skynovel.mail2famibee",
				"title": "Open Mail to famibee",
				"icon": {
					"light": "res/light/openpage.svg",
					"dark": "res/dark/openpage.svg"
				}
			},
			{
				"command": "skynovel.tw2famibee",
				"title": "Open Twitter to famibee",
				"icon": {
					"light": "res/light/openpage.svg",
					"dark": "res/dark/openpage.svg"
				}
			},
			{
				"command": "skynovel.opVSCodeExJa",
				"title": "Open WebSite",
				"icon": {
					"light": "res/light/openpage.svg",
					"dark": "res/dark/openpage.svg"
				}
			},
			{
				"command": "skynovel.opVSCodeExIcon",
				"title": "Open WebSite",
				"icon": {
					"light": "res/light/openpage.svg",
					"dark": "res/dark/openpage.svg"
				}
			},
			{
				"command": "skynovel.opVSCodeExBookmarks",
				"title": "Open WebSite",
				"icon": {
					"light": "res/light/openpage.svg",
					"dark": "res/dark/openpage.svg"
				}
			},
			{
				"command": "skynovel.opVSCodeExLiveHTMLPrev",
				"title": "Open WebSite",
				"icon": {
					"light": "res/light/openpage.svg",
					"dark": "res/dark/openpage.svg"
				}
			},
			{
				"command": "skynovel.opVSCodeExHTMLHint",
				"title": "Open WebSite",
				"icon": {
					"light": "res/light/openpage.svg",
					"dark": "res/dark/openpage.svg"
				}
			},
			{
				"command": "skynovel.opVSCodeExCordovaTools",
				"title": "Open WebSite",
				"icon": {
					"light": "res/light/openpage.svg",
					"dark": "res/dark/openpage.svg"
				}
			},
			{
				"command": "skynovel.opVSCodeExDbg4Chrome",
				"title": "Open WebSite",
				"icon": {
					"light": "res/light/openpage.svg",
					"dark": "res/dark/openpage.svg"
				}
			},
			{
				"command": "skynovel.opVSCodeExglTFTools",
				"title": "Open WebSite",
				"icon": {
					"light": "res/light/openpage.svg",
					"dark": "res/dark/openpage.svg"
				}
			},
			{
				"command": "skynovel.edPrjJson",
				"title": "SKYNovel GUIで編集する"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "skynovel",
					"title": "SKYNovel",
					"icon": "res/icon-activity-bar.svg"
				}
			]
		},
		"views": {
			"skynovel": [
				{
					"id": "sn-setting",
					"name": "開発環境"
				},
				{
					"id": "sn-dev",
					"name": "開発ツール"
				},
				{
					"id": "sn-doc",
					"name": "ドキュメント・連絡先"
				}
			]
		},
		"menus": {
			"view/title": [
				{
					"command": "skynovel.refreshSetting",
					"when": "view == sn-setting",
					"group": "navigation"
				}
			],
			"view/item/context": [
				{
					"command": "skynovel.dlNode",
					"when": "view == sn-setting && viewItem == 'Node.js'",
					"group": "inline"
				},
				{
					"command": "skynovel.opNodeSite",
					"when": "view == sn-setting && viewItem == 'Node.js サイト'",
					"group": "inline"
				},
				{
					"command": "skynovel.devSnUpd",
					"when": "view == sn-dev && viewItem == 'SKYNovel更新'",
					"group": "inline"
				},
				{
					"command": "skynovel.devCrypt",
					"when": "view == sn-dev && viewItem == '暗号化'",
					"group": "inline"
				},
				{
					"command": "skynovel.devTaskWeb",
					"when": "view == sn-dev && viewItem == 'ブラウザ版を起動'",
					"group": "inline"
				},
				{
					"command": "skynovel.devTaskStart",
					"when": "view == sn-dev && viewItem == 'アプリ版を起動'",
					"group": "inline"
				},
				{
					"command": "skynovel.devTaskPackWin",
					"when": "view == sn-dev && viewItem == 'exe生成'",
					"group": "inline"
				},
				{
					"command": "skynovel.devTaskPackMac",
					"when": "view == sn-dev && viewItem == 'app生成（macOS上のみ）'",
					"group": "inline"
				},
				{
					"command": "skynovel.opDev",
					"when": "view == sn-doc && viewItem == '開発者向け情報'",
					"group": "inline"
				},
				{
					"command": "skynovel.opTag",
					"when": "view == sn-doc && viewItem == 'タグリファレンス'",
					"group": "inline"
				},
				{
					"command": "skynovel.opMacroPlg",
					"when": "view == sn-doc && viewItem == 'マクロ・プラグインリファレンス'",
					"group": "inline"
				},
				{
					"command": "skynovel.opGallery",
					"when": "view == sn-doc && viewItem == '機能ギャラリー'",
					"group": "inline"
				},
				{
					"command": "skynovel.dlTmpYoko",
					"when": "view == sn-doc && viewItem == '横書き「初音館にて」'",
					"group": "inline"
				},
				{
					"command": "skynovel.dlTmpTate",
					"when": "view == sn-doc && viewItem == '縦書き「桜の樹の下には」'",
					"group": "inline"
				},
				{
					"command": "skynovel.opFamibeeBlog",
					"when": "view == sn-doc && viewItem == 'famibee blog'",
					"group": "inline"
				},
				{
					"command": "skynovel.mail2famibee",
					"when": "view == sn-doc && viewItem == 'famibee Mail'",
					"group": "inline"
				},
				{
					"command": "skynovel.tw2famibee",
					"when": "view == sn-doc && viewItem == 'famibee Twitter'",
					"group": "inline"
				},
				{
					"command": "skynovel.opVSCodeExJa",
					"when": "view == sn-doc && viewItem == '日本語化'",
					"group": "inline"
				},
				{
					"command": "skynovel.opVSCodeExIcon",
					"when": "view == sn-doc && viewItem == 'Material Icon Theme'",
					"group": "inline"
				},
				{
					"command": "skynovel.opVSCodeExBookmarks",
					"when": "view == sn-doc && viewItem == 'Bookmarks'",
					"group": "inline"
				},
				{
					"command": "skynovel.opVSCodeExLiveHTMLPrev",
					"when": "view == sn-doc && viewItem == 'HTML Preview'",
					"group": "inline"
				},
				{
					"command": "skynovel.opVSCodeExHTMLHint",
					"when": "view == sn-doc && viewItem == 'HTMLHint'",
					"group": "inline"
				},
				{
					"command": "skynovel.opVSCodeExCordovaTools",
					"when": "view == sn-doc && viewItem == 'Cordova Tools'",
					"group": "inline"
				},
				{
					"command": "skynovel.opVSCodeExDbg4Chrome",
					"when": "view == sn-doc && viewItem == 'Debugger for Chrome'",
					"group": "inline"
				},
				{
					"command": "skynovel.opVSCodeExglTFTools",
					"when": "view == sn-doc && viewItem == 'glTF Tools'",
					"group": "inline"
				}
			],
			"explorer/context": [
				{
					"when": "resourceLangId == json",
					"command": "skynovel.edPrjJson",
					"group": "7_modification"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p core/",
		"watch": "tsc -p core/ -w",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"npm_check_updates_do": "ncu -a && npm update && npm i -D",
		"version:patch": "npm -no-git-tag-version version patch",
		"version:minor": "npm -no-git-tag-version version minor",
		"version:major": "npm -no-git-tag-version version major",
		"package": "vsce package",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"dependencies": {
		"crypto-js": "^3.1.9-1",
		"fs-extra": "^8.1.0",
		"image-size": "^0.7.4",
		"uuid": "^3.3.2",
		"xregexp": "^4.2.4"
	},
	"devDependencies": {
		"@types/crypto-js": "^3.1.43",
		"@types/fs-extra": "^8.0.0",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.6.8",
		"@types/uuid": "^3.4.5",
		"@types/xregexp": "^3.0.30",
		"typescript": "^3.5.3",
		"vscode": "^1.1.35"
	},
	"keywords": [
		"skynovel",
		"vscode",
		"vscode-extension",
		"vscode-plugin",
		"typescript",
		"multi-root ready"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/famibee/SKYNovel-vscode-extension.git"
	},
	"bugs": {
		"url": "https://github.com/famibee/SKYNovel-vscode-extension/issues",
		"email": "famibee@gmail.com"
	},
	"homepage": "http://famibee.blog38.fc2.com/"
}
